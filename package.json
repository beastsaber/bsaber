{
  "name": "bsaber",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev:frontend\" \"npm run decap-server\"",
    "dev:frontend": "vite dev",
    "build": "npm run check:meta-headers && envsubst < ./environmentVariables.ts.template > ./src/lib/environmentVariables.ts && vite build && npm run generate:sitemap",
    "generate:sitemap": "svelte-sitemap --domain https://bsaber.com -i '/admin'",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "check:meta-headers": "node ./check-meta-headers.js",
    "lint": "prettier --check . && eslint",
    "format": "prettier --write .",
    "format:lint": "prettier --write . && eslint --fix",
    "create-indices": "node create-indices",
    "server": "node backend/server",
    "decap-server": "npx decap-server"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/svelte-fontawesome": "^0.2.2",
    "@playwright/test": "1.25.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.20.8",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/marked": "^6.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "sharp": "^0.33.2",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-fa": "^2.4.0",
    "svelte-preprocess": "^6.0.0",
    "svelte-sitemap": "^2.6.0",
    "tslib": "^2.3.1",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.4.1"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/dm-sans": "^4.5.9",
    "@fontsource/jetbrains-mono": "^5.0.18",
    "@fontsource/poppins": "^4.5.10",
    "@opensearch-project/opensearch": "^1.0.2",
    "flag-icons": "^7.2.1",
    "front-matter": "^4.0.2",
    "fuse.js": "^7.0.0",
    "js-yaml": "^4.1.0",
    "marked": "^11.2.0",
    "mdsvex": "^0.12.0"
  }
}
